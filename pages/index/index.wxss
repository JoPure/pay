/* pages/index/index.wxss */
page {
  height: 100%;
}

.container {
  height: 100%;
  background-color: #f5f5f5;
}

.chose-yuangong, .chose-shop {
  width: 98%;
  height: 200px;
  margin: 10rpx 1% 10rpx 1%;
  border: 1rpx solid #f5f5f5;
  background-color: #fff;
  display: flex;
  justify-content: center;
  align-items: center;
}

image {
  width: 128rpx;
  height: 128rpx;
}

.chose-yuangong text, .chose-shop text {
  font-size: 18pt;
  margin-left: 20rpx;
}
